{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": [true, 2],
    "no-consecutive-blank-lines": [true, 2]
  },
  "rulesDirectory": []
}